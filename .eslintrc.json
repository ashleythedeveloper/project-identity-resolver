{
    "extends": ["eslint:recommended", "prettier", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "root": true,
    "overrides": [
        {
            "files": ["*.ts"],
            "rules": {
                "@typescript-eslint/no-explicit-any": "warn"
            }
        }
    ]
}
